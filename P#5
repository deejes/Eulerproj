def is_prime(num):
    x = num - 1
    while x > 1:
        if num % x == 0:
            return False
            break
        else:
            x = x - 1
    return True

def factor(num):
    factors = []
    x = num - 1
    while num != 1:
        if num % x == 0 and is_prime(x):
            num = num / x
            factors.append(x)
        else:
            x = x - 1
    return factors

